{{with .Data}}
<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Frameset//EN" "http://www.w3.org/TR/html4/frameset.dtd">
<html>
<head>
	{{if .CustomCSS}}
	<link rel="stylesheet" href="{{.CustomCSS}}">
	{{end}}
	<link rel="stylesheet" href="/static/style.css">
	<meta http-equiv="Content-Type" content="text/html;charset=UTF-8"> 
	<link rel="icon" type="image/png" href="/static/favicon.png">
	<title>{{.Title}}</title>
</head>
<frameset cols="424px,*" {{if $.Ctx.DarkMode}}class="dark"{{end}}>
	<frameset rows="316px,*"{{if $.Ctx.DarkMode}}class="dark"{{end}}>
		<frame name="nav" src="/nav"{{if $.Ctx.DarkMode}}class="dark"{{end}}> 
		<frame name="notification" src="/notifications"{{if $.Ctx.DarkMode}}class="dark"{{end}}> 
	</frameset {{if $.Ctx.DarkMode}}class="dark"{{end}}>
	<frame name="main" src="/timeline/home" {{if $.Ctx.DarkMode}}class="dark"{{end}}> 
</frameset>
</html>
{{end}}
